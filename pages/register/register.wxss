.section {
  width: 100%;
  height: 200rpx;
  justify-content: center;
  align-items: center;
  }
  
.section__title{
  padding: 0 0 0 40rpx;
  color: rgb(0, 119, 255);
  font-size: 30rpx;
  font-weight: 900;
  }
  
.picker{
  padding: 0 0 0 20rpx;
  width: 90%;
  height: 70rpx;
  line-height: 70rpx;
  border: 1rpx solid rgb(0, 119, 255);
  border-radius: 15rpx;
  margin: 0 auto;
  margin-top: 30rpx;  
  color: rgb(0, 119, 255);
}

.navigationBar{
  position:fixed;
  display: flex;
  width: 750rpx;
  height: 200rpx;
  color:#fff;
  z-index: 1;
}